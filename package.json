{
  "name": "jip",
  "version": "0.0.1",
  "private": true,
  "description": "A hangul learning homepage",
  "main": "src/index.js",
  "scripts": {
    "start": "beefy --cwd=public src/index.js:bundle.js",
    "format": "prettier --write src/**/*.js public/index.html",
    "build": "browserify src/index.js | google-closure-compiler -O ADVANCED -W quiet > dist/bundle.js",
    "deploy": "npm run build && cp public/index.html dist/index.html && gh-pages -d dist"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "babelify": "^10.0.0",
    "beefy": "^2.1.8",
    "browserify": "^16.2.3",
    "gh-pages": "^2.0.1",
    "google-closure-compiler": "^20190215.0.1",
    "prettier": "^1.16.4",
    "watchify": "^3.11.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "d3-ease": "^1.0.5",
    "opentype.js": "^0.11.0",
    "paper": "^0.12.0"
  }
}
